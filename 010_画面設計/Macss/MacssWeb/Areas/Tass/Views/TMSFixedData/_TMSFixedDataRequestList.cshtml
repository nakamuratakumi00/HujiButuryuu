@model IEnumerable<Macss.Database.Entity.DummyTMSFixedDataRequest>

@Html.AntiForgeryToken()


<table id="RequestConfirmList" class="table table-striped table-hover macss-data-table">
    <thead class="macss-data-table-header">
        <tr>
            <th>No</th>
            <th>メッセージ</th>
            <th>受付No</th>
            <th>発注No</th>
            <th>顧客伝票No</th>
            <th>顧客コード</th>
            <th>顧客名</th>
            <th>協力会社コード</th>
            <th>協力会社名</th>
            <th>積日</th>
            <th>積地 担当者</th>
            <th>備考（作業依頼）</th>
            <th>品名</th>
            <th>箱数</th>
            <th>貨物重量</th>
            <th>車種コード（発注）</th>
            <th>車種名（発注）</th>
            <th>請求計上状態2</th>
            <th>請求計上状態名2</th>
            <th>支払計上状態2</th>
            <th>支払計上状態名2</th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td style="white-space:nowrap;"></td>
            <td>
                <a tabindex="0" class="btn btn-sm btn-warning" role="button" data-toggle="popover"
                   data-trigger="focus" title="エラー詳細"
                   data-content="@item.UploadErrorMessage">詳細</a>
            </td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
        </tr>
            break;
        }
    </tbody>
</table>
<script>
    $(function () {

        var dtReq = new Macss.DataTables.MacssDataTable("#RequestConfirmList");
        dtReq.configure();

    });
</script>

