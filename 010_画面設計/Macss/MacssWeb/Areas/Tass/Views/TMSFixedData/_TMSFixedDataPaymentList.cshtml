@model IEnumerable<Macss.Database.Entity.DummyTMSFixedDataPayment>

@Html.AntiForgeryToken()

@Html.Partial("_TMSFixedDataList")

<table id="PaymentConfirmList" class="table table-striped table-hover macss-data-table">
    <thead class="macss-data-table-header">
        <tr>
            <th>No</th>
            <th>メッセージ</th>
            <th>仕入No</th>
            <th>発注No</th>
            <th>PCコード1（部門コード）</th>
            <th>PCコード2（事業区分コード）</th>
            <th>PCコード3（請求グループコード）</th>
            <th>協力会社コード</th>
            <th>協力会社名</th>
            <th>税区分</th>
            <th>摘要コード</th>
            <th>外税金額</th>
            <th>支払計上状態2</th>
            <th>支払計上状態名2</th>
            <th>支払距離</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td style="white-space:nowrap;"></td>
            <td>
                <a tabindex="0" class="btn btn-sm btn-warning" role="button" data-toggle="popover"
                   data-trigger="focus" title="エラー詳細"
                   data-content="@item.UploadErrorMessage">詳細</a>
            </td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
            <td></td>
        </tr>
            break;
        }
    </tbody>
</table>
<script>
    $(function () {
        //var dtPay = new Macss.DataTables.MacssDataTable("#PaymentConfirmList", null, 2);
        var dtPay = new Macss.DataTables.MacssDataTable("#PaymentConfirmList");
        dtPay.configure();

    });
</script>
