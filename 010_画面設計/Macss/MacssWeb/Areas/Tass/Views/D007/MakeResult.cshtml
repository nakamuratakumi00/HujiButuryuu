            <th>工場出荷日</th>
            <th>出荷No</th>
            <th>注文主コード</th>
            <th>注文主</th>
            <th>営業担当</th>
            <th>注文番号</th>
            <th>型式仕様</th>
            <th>摘要</th>
            <th>SP単価</th>
            <th>外貨SP単価</th>
            <th>出荷場所</th>
            <th>出荷数</th>
            <th>最終需要家</th>
            <th>受注元</th>
            <th>通貨</th>
            <th>顧客注文番号</th>
            <th>顧客型式</th>
            <th>保留</th>
        </tr>
    </thead>
