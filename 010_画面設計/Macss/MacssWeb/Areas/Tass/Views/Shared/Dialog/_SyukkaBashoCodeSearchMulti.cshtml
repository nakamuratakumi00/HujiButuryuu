<script>
    $('#BtnSelect').on('click', function () {
        var target_cds = [];
        var target_names = [];
        var idx = 0;
        $(".checkSyukkabashoSelect").each(function (i, elem) {
            if ($(elem).prop("checked") == true) {
                var elem_key = $(elem).attr('id').split('_')[1];

                target_cds.push(elem_key);
                if (idx < 2) {
                    target_names.push($("#SyukkabashoVal_" + elem_key).html());
                }
                else if (idx == 2) {
                    target_names.push("…");
                }
                idx = idx + 1;
            }           
        });
        $('#SyukkaBashoCd').val(target_cds.join(','));
        $('#SyukkaBashoName').html(target_names.join(','));
    });
</script>

