
<div class="modal" id="ListSettingDialog" tabindex="-1" role="dialog" aria-labelledby="ListSettingDialogTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="ListSettingDialogTitle">一覧表示設定</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body macss-search-dialog-body">
                <div class="container-fluid">
                    <div class="dataTables_scroll">
                        <div class="dataTables_scrollHead" style="overflow: hidden; position: relative; border: 0px; width: 100%;">
                            <div class="dataTables_scrollHeadInner" style="box-sizing: content-box; width: 403px; padding-right: 17px;">
                                <table class="table table-striped table-hover macss-dialog-data-table dataTable no-footer" style="margin-left: 0px; width: 600px;" role="grid">
                                    <thead class="macss-data-table-header">
                                        <tr role="row">
                                            <th class="" tabindex="0" aria-controls="DepartmentSearchList" rowspan="1" colspan="1" style="width: 200px;">
                                                項目名
                                            </th>
                                            <th class="" tabindex="0" aria-controls="DepartmentSearchList" rowspan="1" colspan="1" style="width: 200px;">
                                                表示状態
                                            </th>
                                        </tr>
                                    </thead>
                                </table>
                            </div>
                        </div><div class="dataTables_scrollBody" style="position: relative; overflow: auto; width: 100%; max-height: 460px;">
                            <table id="DepartmentSearchList" class="table table-striped table-hover macss-dialog-data-table dataTable no-footer" style="width: 600px;" role="grid" aria-describedby="DepartmentSearchList_info">
                                <thead class="macss-data-table-header">
                                    <tr role="row" style="height: 0px;">
                                        <th class="" aria-controls="DepartmentSearchList" rowspan="1" colspan="1" style="width: 200px; padding-top: 0px; padding-bottom: 0px; border-top-width: 0px; border-bottom-width: 0px; height: 0px;">
                                            <div class="dataTables_sizing" style="height:0;overflow:hidden;">
                                                項目名
                                            </div>
                                        </th>
                                        <th class="" aria-controls="DepartmentSearchList" rowspan="1" colspan="1" style="width: 200px; padding-top: 0px; padding-bottom: 0px; border-top-width: 0px; border-bottom-width: 0px; height: 0px;">
                                            <div class="dataTables_sizing" style="height:0;overflow:hidden;">
                                                表示状態
                                            </div>
                                        </th>
                                    </tr>
                                </thead>

                                <tbody>
                                    <tr role="row" class="odd">
                                        <td>出荷日</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible1" value="1" id="inlineRadioShow1" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow1">表示</label>
                                                <input type="radio" name="colVisible1" value="0" id="inlineRadioHide1">
                                                <label class="form-check-label ml-1" for="inlineRadioHide1">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>対象年月</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible2" value="1" id="inlineRadioShow2" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow2">表示</label>
                                                <input type="radio" name="colVisible2" value="0" id="inlineRadioHide2">
                                                <label class="form-check-label ml-1" for="inlineRadioHide2">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>出荷No</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible3" value="1" id="inlineRadioShow3" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow3">表示</label>
                                                <input type="radio" name="colVisible3" value="0" id="inlineRadioHide3">
                                                <label class="form-check-label ml-1" for="inlineRadioHide3">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>得意先コード</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible4" value="1" id="inlineRadioShow4" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow4">表示</label>
                                                <input type="radio" name="colVisible4" value="0" id="inlineRadioHide4">
                                                <label class="form-check-label ml-1" for="inlineRadioHide4">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>得意先名</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible5" value="1" id="inlineRadioShow5" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow5">表示</label>
                                                <input type="radio" name="colVisible5" value="0" id="inlineRadioHide5">
                                                <label class="form-check-label ml-1" for="inlineRadioHide5">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>振替得意先コード</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible6" value="1" id="inlineRadioShow" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow">表示</label>
                                                <input type="radio" name="colVisible6" value="0" id="inlineRadioHide">
                                                <label class="form-check-label ml-1" for="inlineRadioHide">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>出荷統合コード</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible7" value="1" id="inlineRadioShow" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow">表示</label>
                                                <input type="radio" name="colVisible7" value="0" id="inlineRadioHide">
                                                <label class="form-check-label ml-1" for="inlineRadioHide">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>荷札発行済フラグ</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible8" value="1" id="inlineRadioShow" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow">表示</label>
                                                <input type="radio" name="colVisible8" value="0" id="inlineRadioHide">
                                                <label class="form-check-label ml-1" for="inlineRadioHide">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>経費負担No</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible9" value="1" id="inlineRadioShow" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow">表示</label>
                                                <input type="radio" name="colVisible9" value="0" id="inlineRadioHide">
                                                <label class="form-check-label ml-1" for="inlineRadioHide">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>Fe機種コード</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible10" value="1" id="inlineRadioShow" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow">表示</label>
                                                <input type="radio" name="colVisible10" value="0" id="inlineRadioHide">
                                                <label class="form-check-label ml-1" for="inlineRadioHide">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>オリジナル出荷No</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible11" value="1" id="inlineRadioShow" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow">表示</label>
                                                <input type="radio" name="colVisible11" value="0" id="inlineRadioHide">
                                                <label class="form-check-label ml-1" for="inlineRadioHide">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>届先郵便番号</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible12" value="1" id="inlineRadioShow" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow">表示</label>
                                                <input type="radio" name="colVisible12" value="0" id="inlineRadioHide">
                                                <label class="form-check-label ml-1" for="inlineRadioHide">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    <tr role="row" class="odd">
                                        <td>届先住所</td>
                                        <td>
                                            <div class="form-check form-check-inline">
                                                <input type="radio" name="colVisible13" value="1" id="inlineRadioShow" checked="checked">
                                                <label class="form-check-label ml-1 mr-4" for="inlineRadioShow">表示</label>
                                                <input type="radio" name="colVisible13" value="0" id="inlineRadioHide">
                                                <label class="form-check-label ml-1" for="inlineRadioHide">非表示</label>
                                            </div>
                                        </td>
                                    </tr>
                                    @for (int i = 0; i < 10; i++)
                                    {
                                        <tr role="row" class="odd">
                                            <td></td>
                                            <td>
                                                <div class="form-check form-check-inline">
                                                    <input type="radio" name="colVisible" value="1" id="inlineRadioShow" checked="checked">
                                                    <label class="form-check-label ml-1 mr-4" for="inlineRadioShow">表示</label>
                                                    <input type="radio" name="colVisible" value="0" id="inlineRadioHide">
                                                    <label class="form-check-label ml-1" for="inlineRadioHide">非表示</label>
                                                </div>
                                            </td>
                                        </tr>
                                    }

                                </tbody>
                            </table>
                        </div>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <div class="row">
                    <div class="col-4">
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">@MacssWeb.Resources.ControlText.Cancel</button>
                    </div>
                    <div class="col-8 text-right">
                        <button type="button" class="btn btn-primary" data-dismiss="modal">設定</button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
