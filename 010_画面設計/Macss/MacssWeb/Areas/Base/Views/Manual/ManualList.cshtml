                            <label class="form-check-label" for="inlineRadio2">社外用</label>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
}

@section Scripts {
    <script>
        $(function () {

            for (var i = 1; i <= 11; i++) {
                var acc = "#MenuAccordionTass" + i;
                var shw = "#MenuShowTass" + i;
                var hdn = "#MenuHiddenTass" + i;

                var cond = new Macss.Collapse.MenuBar(acc, shw, hdn);
                cond.setCollapseEvent();
            }

            for (var i = 2; i <= 6; i++) {
                var acc = "#MenuAccordion" + i;
                var shw = "#MenuShow" + i;
                var hdn = "#MenuHidden" + i;

                var cond = new Macss.Collapse.MenuBar(acc, shw, hdn);
                cond.setCollapseEvent();
            }
        });
    </script>
}
