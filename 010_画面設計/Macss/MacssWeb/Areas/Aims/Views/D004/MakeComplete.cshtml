<html lang="ja">
</html>
@{
    ViewBag.PageMainTitle = "請求明細データ作成結果(末締1手番)";
}
@section Scripts {
    <script>
        $(function () {
            $('[data-toggle="popover"]').popover();

            var dt = new Macss.DataTables.MacssSimpleTable("#SampleList");
            dt.configure();

        });</script>
}

<div class="alert @ViewBag.AlertType">
    <span>@ViewBag.ResultMessage</span>
</div>
<div class="row justify-content-end macss-form-button-group-row">
    <div class="col-4 text-right">
        <a role="button" class="btn btn-secondary" href="@Url.Action("Index", "Home")">サブメニューへ戻る</a>
    </div>
</div>

<div class="container-fluid">
    
    <div class="form-inline">                               
        <div class="input-group mr-2 mb-1">
            <div class="input-group-prepend">
                <span class="input-group-text">請求明細データ作成件数</span>
            </div>
            <span class="form-control">50件</span>
        </div>
        <div class="input-group mr-2 mb-1">
            <div class="input-group-prepend">
                <span class="input-group-text">請求引渡データ作成件数</span>
            </div>
            <span class="form-control">50件</span>
        </div>
    </div>    
</div>

