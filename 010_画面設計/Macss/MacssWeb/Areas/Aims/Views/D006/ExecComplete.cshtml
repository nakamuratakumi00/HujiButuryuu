<html lang="ja">
</html>

@{
    ViewBag.Title = "請求売上・仕入確定";
    ViewBag.PageMainTitle = "請求売上・仕入確定 完了";
}

@section Scripts {
    <script>
        $(function () {
            $('[data-toggle="popover"]').popover();

            var dt = new Macss.DataTables.MacssSimpleTable("#ExecCompleteList");
            dt.configure();

        });
    </script>
}

<div class="alert @ViewBag.AlertType">
    <span>@ViewBag.ResultMessage</span>
</div>

<div class="row justify-content-between macss-form-button-group-row">
    <div class="col-4">
    </div>
    <div class="col-4 text-right">
        <a role="button" class="btn btn-secondary" href="@Url.Action("Index", "Home")">@MacssWeb.Resources.ControlText.BackToSubMenu</a>
    </div>
</div>

@using (Html.BeginForm("ExecComplete", "Aims", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    @Html.Partial("_Exec")
}

<br />

<div class="mx-lg-0">

    <table id="ExecCompleteList" class="table table-striped table-hover macss-data-table">
        <thead class="macss-data-table-header">
            <tr>
                <th>
                    更新データ　　　　
                </th>
                <th>
                    更新件数
                </th>
            </tr>
        </thead>
        <tbody>
            <tr>
                <td>請求明細データ</td>
                <td>100件</td>
            </tr>
            <tr>
                <td>売上伝票データ</td>
                <td>0件</td>
            </tr>
        </tbody>
    </table>
</div>