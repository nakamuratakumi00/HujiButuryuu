
<div class="modal" id="HinmeiCodeSearchDialog" tabindex="-1" role="dialog" aria-labelledby="HinmeiCodeSearchDialogTitle" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="HinmeiCodeSearchDialogTitle">品名コード検索</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body macss-search-dialog-body">
                <div class="container-fluid">
                    <div class="row">
                        <div class="col-6">
                            <div class="input-group mb-3">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="Condition1">コード</span>
                                </div>
                                <input type="text" id="CondHinmeiCode" class="form-control" aria-label="Condition1" aria-describedby="Condition1">
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="input-group mb-3">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="Condition1">名称</span>
                                </div>
                                <input type="text" id="CondHinmeiName" class="form-control" aria-label="Condition1" aria-describedby="Condition1">
                            </div>
                        </div>
                    </div>
                    <div class="row">
                        <div class="col-6">
                            <div class="input-group mb-3">
                                <div class="input-group-prepend">
                                    <span class="input-group-text" id="Condition1">カナ</span>
                                </div>
                                <input type="text" id="CondHinmeiNameKana" class="form-control" aria-label="Condition1" aria-describedby="Condition1">
                            </div>
                        </div>
                        <div class="col-6 text-right">
                            <button type="button" class="btn btn-primary" onclick="searchDeparatments($('#CondTodokesakiCode').val(), $('#CondTodokesakiName').val())">
                                @MacssWeb.Resources.ControlText.Search
                            </button>
                        </div>
                    </div>
                </div>
                <div>
                    <table id="HinmeiList" class="table table-striped table-hover macss-dialog-data-table">
                        <thead class="macss-data-table-header">
                            <tr>
                                <th>　　</th>
                                <th>
                                    コード
                                </th>
                                <th>
                                    名称　　　　　　　　　　　　　　　
                                </th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr role="row">
                                <td style="white-space:nowrap;"><button type="button" class="btn btn-outline-secondary btn-sm" onclick="' + selectEvent + '">@MacssWeb.Resources.ControlText.Select</button></td>
                                <td>001001</td>
                                <td>ユニット</td>
                            </tr>
                            <tr role="row">
                                <td style="white-space:nowrap;"><button type="button" class="btn btn-outline-secondary btn-sm" onclick="' + selectEvent + '">@MacssWeb.Resources.ControlText.Select</button></td>
                                <td>001025</td>
                                <td>電算機用部品</td>
                            </tr>
                            <tr role="row">
                                <td style="white-space:nowrap;"><button type="button" class="btn btn-outline-secondary btn-sm" onclick="' + selectEvent + '">@MacssWeb.Resources.ControlText.Select</button></td>
                                <td>001035</td>
                                <td>リードフレーム</td>
                            </tr>
                            <tr role="row">
                                <td style="white-space:nowrap;"><button type="button" class="btn btn-outline-secondary btn-sm" onclick="' + selectEvent + '">@MacssWeb.Resources.ControlText.Select</button></td>
                                <td>001045</td>
                                <td>端子ブロック</td>
                            </tr>
                            <tr role="row">
                                <td style="white-space:nowrap;"><button type="button" class="btn btn-outline-secondary btn-sm" onclick="' + selectEvent + '">@MacssWeb.Resources.ControlText.Select</button></td>
                                <td>001048</td>
                                <td>プリントバン</td>
                            </tr>
                            @for (int i = 0; i < 100; i++)
                            {
                                <tr>
                                    <td style="white-space:nowrap;">
                                        <button type="button" class="btn btn-outline-secondary btn-sm" onclick="' + selectEvent + '">@MacssWeb.Resources.ControlText.Select</button>
                                    </td>
                                    <td></td>
                                    <td></td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">@MacssWeb.Resources.ControlText.Cancel</button>
            </div>
        </div>
    </div>
</div>
<script>
    let HinmeiiListTable;
    var colDef =
        [

        ];
    $(function () {
        HinmeiiListTable = new Macss.DataTables.MacssSearchDialogTable("#HinmeiList", colDef);
        HinmeiiListTable.configure();
        $('#HinmeiCodeSearchDialog').on('shown.bs.modal', function (e) {
            //dt.configure(colDef);
        })
    });
</script>
